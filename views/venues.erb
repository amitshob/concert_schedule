

<form action="/venues" method="post">
  <label for="venue">Venue Name:</label>
  <input id="venue" name="venue" type="text">

  <button type="submit" class="btn btn-success">Add Venue</button>
</form>


<ul>
  <% if @venue %>
    <% @venue.each() do |venue| %>
    <li><a href="/venues/<%= venue.id %>"><%= venue.name() %></a></li>
    <% end %>
  <% end %>
</ul>




<a href="/"> HOME </a>
